+        Chat chat=mChat.get(position);
+        holder.showMessage.setText(chat.getMessage());
+        holder.profileImage.setImageResource(R.mipmap.ic_launcher);
+    }
+
+    @Override
+    public int getItemCount() {
+        return mChat.size();
+    }
+
