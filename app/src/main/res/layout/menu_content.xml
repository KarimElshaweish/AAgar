<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center">
    <LinearLayout
        android:padding="20dp"
        android:id="@+id/lin"
        android:layout_marginRight="10dp"
        android:weightSum="10"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
            <TextView
                android:id="@+id/order"
                android:layout_gravity="center"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:textSize="17dp"
                android:drawablePadding="7dp"
                android:textColor="#7D7D7D"
                android:fontFamily="@font/gess_two_medium"
                android:text="الطلبات النشطه"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>

<TextView
        android:id="@+id/activeOrder"
    android:layout_gravity="center"
    android:gravity="center_vertical"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="1"
    android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text="عروضى "/>
        <View
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
    android:id="@+id/soldOrder"
    android:layout_gravity="center"
    android:gravity="center_vertical"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="1"
    android:textSize="17dp"
            android:visibility="gone"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text=" عروضى المباعة"/>

        <TextView
            android:id="@+id/fav"
            android:gravity="center_vertical"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#000"
    android:fontFamily="@font/vip"
    android:drawableRight="@drawable/ic_orders_fav"
    android:text="المفضله"/>

            <TextView
            android:id="@+id/notifcation_nav"
            android:gravity="center_vertical"

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text="الإشعارات"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:id="@+id/profile_nav"
            android:gravity="center_vertical"

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text="الملف الشخصى"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:id="@+id/chat_nav"
            android:gravity="center_vertical"

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text="المحادثات"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:id="@+id/myWallet"
            android:gravity="center_vertical"

            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text=" محفظتى"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/moneyGetWayText"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
             android:drawablePadding="7dp"
              android:textColor="#7D7D7D"
              android:fontFamily="@font/gess_two_medium"
                 android:text="طرق الدفع"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:gravity="center_vertical"
android:id="@+id/feedback"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#7D7D7D"
    android:fontFamily="@font/gess_two_medium"
    android:text="الشكاوى"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#828282"/>
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/txtLogout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:textSize="17dp"
    android:drawablePadding="7dp"
    android:textColor="#E12929"
    android:fontFamily="@font/gess_two_medium"
    android:text="تسجيل الخروج"/>
    </LinearLayout>
    <ImageView
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:src="@drawable/bttm_layer"/>
</RelativeLayout>